
{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "standard"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules" {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-unused-vars": "warn"
    }
}
